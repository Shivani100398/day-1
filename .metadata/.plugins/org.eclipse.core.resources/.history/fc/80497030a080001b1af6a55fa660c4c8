
public class Teacher {

	private String name;
	private String subject;
	
	Teacher(String name){
		
		setName(name);
	//	setSubject(sub);
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	
	
	
}
